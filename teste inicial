def fizzbuzz(n):
        
   if n % 5 == 0 and n % 3 == 0:
        print ("True 1")
        return 'FizzBuzz'
   else:
    
        if n % 5 != 0 and n % 3 == 0:
             print ("True 2")
             return 'Fizz'
        else:
             if n % 5 == 0 and n % 3 != 0:
                  return 'Buzz'
             else:
                  return n
